{
    "name": "nettsite/taxonomies",
    "description": "Simple, nestable Terms & Taxonomies (similar to WordPress) for Laravel",
    "homepage" : "https://github.com/NettSite/Taxonomies",
    "license": "MIT",
    "keywords": [
        "taxonomies",
        "terms",
        "categories",
        "nestable",
        "eloquent",
        "model",
        "laravel"
    ],
    "authors": [
        {
            "name"     : "Alexander Manfred Poellmann",
            "homepage" : "http://twitter.com/AMPoellmann",
            "role"     : "developer"
        }
    ],
    "require": {
        "php"                 : "^7.3|^8.0",
        "laravel/framework": "^8.12",
        "illuminate/support"  : ">=5.3",
        "illuminate/database" : ">=5.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^9.3.3",
        "doctrine/dbal": "^2.12"
    },
    "autoload": {
        "psr-4": {
            "NettSite\\Taxonomies\\" : "src/"
        }
    },
    "extra": {
        "branch-alias" : {
            "dev-master" : "0.0.x-dev"
        }
    },
    "support" : {
        "source" : "https://github.com/NettSite/Laravel-Taxonomies",
        "issues" : "https://github.com/NettSite/Laravel-Taxonomies/issues"
    }
}
